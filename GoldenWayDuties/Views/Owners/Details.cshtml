@model GoldenWayDuties.Models.Owner
@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>Membership Type: @Model.ResidentType.Name</li>
    @if (Model.DateOfBirth.HasValue)
    {
        <li>@Model.DateOfBirth.Value.ToShortDateString()</li>
    }


</ul> 
